<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="pk.edu.szabist.blogapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:networkSecurityConfig="@xml/network_security_config">
        <activity android:name="pk.waseem.BlogApp.AddOrUpdateBlogActivity"></activity>
        <activity android:name="pk.waseem.BlogApp.ViewMyBlogsActivity" />
        <activity android:name="pk.waseem.BlogApp.ViewAllBlogsActivity" />
        <activity android:name="pk.waseem.BlogApp.HomeActivity" />
        <activity android:name="pk.waseem.BlogApp.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>



</manifest>